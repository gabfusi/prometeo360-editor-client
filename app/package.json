{
  "name": "Prometeo360",
  "description": "Prometeo 360 Ã¨ un editor di montaggio per rendere interattivi video a 360 gradi.",
  "version": "0.1.0",
  "private": false,
  "homepage": "http://prometeo360.gabrielefusi.com",
  "license": "CC-BY-NC-SA-4.0",
  "author": {
    "name": "Gabriele Fusi",
    "email": "gabfusi@gmail.com"
  },
  "repository": {
    "url": "https://github.com/gabfusi/prometeo360-editor-client"
  },
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --enable-logging",
    "pack": "electron-builder --dir --mac --win",
    "dist": "electron-builder --mac --win",
    "dist:mac": "electron-builder --mac",
    "dist:win": "electron-builder --win",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "debug": "~2.6.8",
    "electron-notifications": "^1.0.0",
    "ffmpeg-static": "^2.0.0",
    "ffprobe-static": "^2.0.0",
    "fluent-ffmpeg": "^2.1.0",
    "hbs": "~4.0.0",
    "lowdb": "^0.16.0",
    "morgan": "~1.8.2",
    "node-ipc": "^9.0.1",
    "path": "^0.12.7",
    "request": "^2.81.0",
    "socket.io-client": "^2.0.3",
    "socket.io-stream": "^0.9.1",
    "username": "^3.0.0",
    "uuid-lib": "0.0.6",
    "vrview": "https://github.com/gabfusi/vrview/tarball/master"
  },
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "~1.6.2",
    "electron-builder": "^19.16.0",
    "electron-icon-maker": "0.0.3",
    "electron-packager": "^8.7.2",
    "electron-reload": "^1.1.0"
  },
  "build": {
    "appId": "com.gabfusi.prometeo360",
    "productName": "Prometeo 360",
    "compression": "maximum",
    "directories": {
      "output": "../dist"
    },
    "asarUnpack": [
      "**/app/assets/*",
      "**/app/node_modules/ffmpeg-static/*",
      "**/app/node_modules/ffprobe-static/*"
    ],
    "mac": {
      "category": "public.app-category.video",
      "target": "dmg",
      "icon": "assets/icons/mac/icon.icns"
    },
    "dmg": {
      "contents": [
        {
          "x": 355,
          "y": 125,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 155,
          "y": 125,
          "type": "file"
        }
      ]
    },
    "win": {
      "icon": "assets/icons/win/icon.ico"
    }
  }
}
